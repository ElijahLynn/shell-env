#!/usr/bin/expect -f 

eval spawn -noecho $argv
expect {
    -timeout 3
    -re "Password for .*: " {
        stty -echo
        interact {
            "\r" {
                send "\n"
                return
            }
        }
        exp_continue
    }
    "root]#" {
        send "/home/aspiers/bin/zsh\n"
        interact
    }

    # Match prompts here
    {\$} interact

    timeout interact
    eof
}
